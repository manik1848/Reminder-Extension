{
  "manifest_version": 2,
  "version": "1.0",
  "name": "One Touch",
  "description": "The extension contains links of the most commonly used websites, a Note Taker and an Alarm Reminder.",
  "icons": {
    "16": "icon128.png",
    "48": "icon128.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "One Touch"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "notifications"
  ]
}
